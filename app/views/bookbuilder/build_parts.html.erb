<h1>Bookbuilder#build_parts</h1>
<p>Find me in app/views/bookbuilder/build_parts.html.erb</p>
<%= link_to "Build Layouts", bookbuilder_build_layouts_path, class: "btn btn-info btn-lg" %> 


<% if(@page_parts != nil) %>
	<table>
		<% @books.each do |book| %>
			<tr>
				<td><%= book.name %></td>
				<td><%= book.page_count %></td>
			</tr>
		<% end %>
	</table>
<% end %>