<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
	<div class="panel panel-default">
		<div class="panel-heading accord-btn" role="tab" id="headingOne">
			<h4 class="panel-title">
				<a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
					Library of Contracts 
				</a>
			</h4>
		</div>

	    <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">	<% counter = 1 %>
	    	<div class="panel-body panel-wide">
	 			<% @contracts.each do |z| %>
					<div class="panel_selection panel-default " id="page_<%= z.id %>" >
						<div class="col-sm-3 col-md-2  page-selection" style="width:110px;">
							<div class="panel panel-default panel-page">
								<%  %>
		 						<div class="panel panel-heading caption min-head">
									<p class="pg-title"><span id="page-id"><%= z.id %></span> </p>  
								</div>
								<div class="panel-body panel-button clicked" id="btn-clicked">
			    					
			    					<div class="viewer-image">
			    						<div class="thumbnail">
											<%= link_to pdf_page_index_path(page: z.id, "create_pages" =>  @record_page_set, rec_id: @my_record,) do %>
												<%=  image_tag "pf_base_btn_set/#{counter}.jpg", width: 136, height: 67, alt: ('PF Contract form : ' + z.name) %>
											<% end %> 
					    			    	<% counter = counter + 1 %>
				    					</div>
									</div>
									<%= link_to "Add", pdf_page_index_path(rec_id: @my_record, page: z.id, "create_pages" =>  @record_page_set), class: "btn btn-success pull-left", id: counter - 1  
									%>

									<%= link_to "Remove", pdf_page_index_path(rec_id: @my_record, remove: z.id, "create_pages" =>  @record_page_set), class: "btn btn-warning pull-right" 
									%>	
									
								</div>
							</div>
						</div>
					</div>
				<% end %>
		    </div>
	    </div>
	</div>



	  <div class="panel panel-default">
	    <div class="panel-heading" role="tab" id="headingTwo">
	      <h4 class="panel-title">
	        <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
	         Book 1 <%= @books[0].name %>
	         	<div class="btn">VIEW</div>
         			<%= link_to "Fill Entire Book", pdf_page_index_path(rec_id: @my_record, "create_pages" =>  @keepage.to_s +  "1, 3, " ), class: "btn btn-success pull-right" 
					%>
	        </a>
	      </h4>
	    </div>
	    <div id="collapseTwo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingTwo"><% counter = 1 %>
	    	<div class="panel-body">
				<% @contractA.each do |z| %>
					<div class="panel_selection panel-default panel-page">
						<div class="col-sm-3 col-md-2  page-selection">
							<div class="panel panel-default panel-page">
				 				<div class="panel panel-heading caption min-head">
									<h6><%= z.id %> <%= z.name[0..24] %></h6>  
								</div>
								<div class="panel-body panel-button clicked" id="btn-clicked">
			    					
			    					<div class="viewer-image">
			    						<div class="thumbnail">
											<%= link_to pdf_page_index_path(page: z.id, "create_pages" =>  @record_page_set, rec_id: @my_record,) do %>
												<%=  image_tag "pf_base_btn_set/#{z.id}.jpg", width: 136, height: 67, alt: ('PF Contract form : ' + z.name) %>
											<% end %> 
					    			    	<% counter = counter + 1 %>
				    					</div>
									</div>
									<%= link_to "Add", pdf_page_index_path(rec_id: @my_record, page: z.id, "create_pages" =>  @record_page_set), class: "btn btn-success pull-left", id: counter - 1  
									%>

									<%= link_to "Remove", pdf_page_index_path(rec_id: @my_record, remove: z.id, "create_pages" =>  @record_page_set), class: "btn btn-warning pull-right" 
									%>	
									
								</div>
							</div>
						</div>
					</div>
				<% end %>
			</div>
		</div>
	</div>





